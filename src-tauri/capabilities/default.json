{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "EzPlist 默认权限配置",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:default",
    "fs:default",
    {
      "identifier": "fs:allow-read-file",
      "allow": [
        { "path": "**/*" }
      ]
    },
    {
      "identifier": "fs:scope",
      "allow": [
        { "path": "**/*" }
      ]
    }
  ]
}
